import io;

function int add_one(int a) {
    return a + 1;
}

function int operate(int number, func f) {
    number = f(number);
    return number;
}

function void main() {
    let int x = 0;
    let func increment = add_one;
    print(increment);
    x = increment(x);
    let int y = x;
    print(y);
    y = operate(y, increment);
    print(y);
}