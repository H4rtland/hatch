function void print(int n) {
    __internal_print(n);
}

function int triangle_number(int n) {
    if (n == 1) {
        return 1;
    } else {
        return n + triangle_number(n-1);
    }
}

function void main() {
    let int a = 3;
    let int b = triangle_number(10);
    print(a+b);
    let int y = triangle_number(125);
    print(y);
}