import io;

struct Car {
    int wheels,
    int seats,
}

function int wheels_plus_seats(Car car) {
    return car.wheels + car.seats;
}

function void main() {
    let Car ford = new Car(4, 5);
    io.print(ford.wheels);
    io.print(ford.seats);
    io.print(wheels_plus_seats(ford));
    io.print(ford.wheels == 4);
}