function int plus_one(int n) {
    return n + 1;
}

function int get_4() {
    return 4;
}

function int random_integer() {
    // guaranteed random. chosen by fair dice roll.
    let int return_value = get_4();
    return return_value;
}

function void main() {
    let int x = 1;
    x = 16;
    let int y = 4;
    y = plus_one(5);
    print(y);
    y = x + y + random_integer();
    print(x, y);
}